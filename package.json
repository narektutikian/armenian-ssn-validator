{
  "name": "armenian-ssn-validator",
  "version": "0.1.3",
  "description": "Validate and generate Armenian social security numbers (SSNs) in TypeScript for frontend and backend use.",
  "type": "module",
  "main": "dist/cjs/index.js",
  "module": "dist/esm/index.js",
  "types": "dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": "./dist/esm/index.js",
      "require": "./dist/cjs/index.js",
      "types": "./dist/esm/index.d.ts"
    }
  },
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsc -p tsconfig.esm.json && tsc -p tsconfig.cjs.json",
    "test": "vitest run",
    "lint": "tsc --noEmit -p tsconfig.test.json"
  },
  "keywords": [
    "armenia",
    "ssn",
    "social-security-number",
    "validator",
    "typescript"
  ],
  "author": "Narek Tutikyan",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/narektutikian/armenian-ssn-validator.git"
  },
  "bugs": {
    "url": "https://github.com/narektutikian/armenian-ssn-validator/issues"
  },
  "homepage": "https://github.com/narektutikian/armenian-ssn-validator#readme",
  "private": false,
  "devDependencies": {
    "@types/node": "^22.0.0",
    "typescript": "^5.6.0",
    "vitest": "^1.6.0"
  }
}
